<% include partials/header %>

<h1>Blogs</h1>

<% blogs.forEach(function(blog){ %>
    <div>
        <h2><%= blog.title %> </h2>
        <img src="<%= blog.img %>">
        <p><%= blog.body.substring(0, 100) %></p>
        <p><%= blog.created %></p>
        <a href="blogs/<%= blog._id %>">Read more</a>
    </div>

<% }) %>

<% include partials/footer %>
